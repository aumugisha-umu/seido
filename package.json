{
  "name": "seido-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev 2>&1 | npx pino-pretty --colorize --translateTime \"HH:MM:ss\" --ignore \"pid,hostname\"",
    "dev:json": "next dev",
    "dev:pretty": "next dev 2>&1 | npx pino-pretty --colorize --translateTime \"HH:MM:ss\" --ignore \"pid,hostname\"",
    "dev:pretty:full": "next dev 2>&1 | npx pino-pretty --colorize --translateTime \"HH:MM:ss\" --messageFormat \"{msg}\" --ignore \"pid,hostname\"",
    "dev:utf8": "chcp 65001 && npm run dev",
    "dev:win": "powershell -Command \"[Console]::OutputEncoding = [System.Text.Encoding]::UTF8; npm run dev\"",
    "dev:no-emoji": "next dev 2>&1 | npx pino-pretty --colorize --translateTime \"HH:MM:ss\" --ignore \"pid,hostname\"",
    "lint": "next lint",
    "start": "next start",
    "supabase:types": "supabase gen types typescript > lib/database.types.ts",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:migrate": "supabase migration new",
    "migrate:intervention-status": "tsx scripts/migrate-intervention-statuses.ts",
    "migrate:pino-logger": "tsx scripts/migrate-to-pino-logger.ts",
    "migrate:pino-services": "tsx scripts/migrate-services-to-pino.ts",
    "migrate:pino-components": "tsx scripts/migrate-components-to-pino.ts",
    "migrate:logger-format": "tsx scripts/migrate-logger-to-pino-format.ts",
    "migrate:logger-format:write": "tsx scripts/migrate-logger-to-pino-format.ts --write",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run __tests__/unit",
    "test:integration": "vitest run __tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:ci": "vitest run && playwright test",
    "analyze:bundle": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json"
  },
  "dependencies": {
