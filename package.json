{
  "name": "seido-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev 2>&1 | npx pino-pretty --colorize --translateTime \"HH:MM:ss\" --ignore \"pid,hostname\"",
    "dev:json": "next dev",
    "lint": "next lint",
    "start": "next start",
    "supabase:types": "supabase gen types typescript > lib/database.types.ts",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:migrate": "supabase migration new",
    "migrate:intervention-status": "tsx scripts/migrate-intervention-statuses.ts",
    "migrate:pino-logger": "tsx scripts/migrate-to-pino-logger.ts",
    "migrate:pino-services": "tsx scripts/migrate-services-to-pino.ts",
    "migrate:pino-components": "tsx scripts/migrate-components-to-pino.ts",
    "migrate:logger-format": "tsx scripts/migrate-logger-to-pino-format.ts",
    "migrate:logger-format:write": "tsx scripts/migrate-logger-to-pino-format.ts --write",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose lib/",
    "test:components": "vitest run --reporter=verbose components/",
    "test:integration": "vitest run --reporter=verbose test/integration/",
    "test:api": "vitest run --reporter=verbose test/api/",
    "test:security": "vitest run --reporter=verbose test/security/",
    "test:e2e": "playwright test",
    "test:e2e:gestionnaire": "playwright test --project=gestionnaire test/e2e/gestionnaire",
    "test:e2e:prestataire": "playwright test --project=prestataire test/e2e/prestataire",
    "test:e2e:locataire": "playwright test --project=locataire test/e2e/locataire",
    "test:e2e:admin": "playwright test --project=admin test/e2e/admin",
    "test:baseline": "playwright test test/e2e/baseline/ --reporter=html",
    "analyze:bundle": "ANALYZE=true npm run build",
    "agent:tester:baseline": "tsx test/scripts/run-phase-tests.ts --phase=baseline --report",
    "test:e2e:mobile": "playwright test --project=mobile",
    "test:e2e:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:e2e:intervention-flow": "playwright test test/e2e/intervention-lifecycle.spec.ts",
    "test:performance": "playwright test --config=playwright.config.performance.ts",
    "test:accessibility": "playwright test test/e2e/accessibility.spec.ts",
    "test:responsive": "playwright test test/e2e/responsive.spec.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:ci": "npm run test:unit && npm run test:components && npm run test:api && npm run test:e2e",
    "test:full": "npm run test:ci && npm run test:performance && npm run test:accessibility",
    "agent:tester": "tsx test/scripts/run-phase-tests.ts",
    "agent:tester:phase2": "tsx test/scripts/run-phase-tests.ts --phase=phase2 --compare-baseline --report",
    "agent:tester:phase3": "tsx test/scripts/run-phase-tests.ts --phase=phase3 --compare-baseline --report",
    "test:phase3": "tsx test/scripts/run-phase3-tests.ts",
    "test:e2e:auth": "playwright test --config=playwright.config.auth.ts",
    "test:e2e:auth:chrome": "playwright test --config=playwright.config.auth.ts --project=chromium-desktop",
    "test:e2e:auth:mobile": "playwright test --config=playwright.config.auth.ts --project=mobile-chrome --project=mobile-safari",
    "test:e2e:auth:all-browsers": "playwright test --config=playwright.config.auth.ts --project=chromium-desktop --project=firefox-desktop --project=webkit-desktop",
    "test:e2e:auth:report": "playwright show-report playwright-report-auth",
    "agent:tester:final": "tsx test/scripts/run-phase-tests.ts --phase=final --report",
    "agent:tester:workflow": "tsx test/scripts/run-phase-tests.ts --workflow=intervention-complete-workflow --report",
    "test:new": "playwright test --config=tests-new/config/playwright.config.ts",
    "test:new:auth": "playwright test tests-new/auth/ --config=tests-new/config/playwright.config.ts",
    "test:new:contacts": "playwright test tests-new/contacts/ --config=tests-new/config/playwright.config.ts",
    "test:new:signup": "playwright test tests-new/auth/signup.spec.ts --config=tests-new/config/playwright.config.ts",
    "test:new:login": "playwright test tests-new/auth/login.spec.ts --config=tests-new/config/playwright.config.ts",
    "test:new:logout": "playwright test tests-new/auth/logout.spec.ts --config=tests-new/config/playwright.config.ts",
    "test:new:password-reset": "playwright test tests-new/auth/password-reset.spec.ts --config=tests-new/config/playwright.config.ts",
    "test:new:create-contact": "playwright test tests-new/contacts/create-contact.spec.ts --config=tests-new/config/playwright.config.ts",
    "test:new:headed": "HEADED=true playwright test --config=tests-new/config/playwright.config.ts",
    "test:new:headless": "HEADLESS=true playwright test --config=tests-new/config/playwright.config.ts",
    "test:new:report": "playwright show-report tests-new/logs/playwright-report",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@react-email/components": "^0.5.5",
    "@react-email/render": "^1.3.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.0",
    "@vercel/analytics": "latest",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "dataloader": "^2.2.3",
    "date-fns": "4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "8.5.1",
    "geist": "latest",
    "input-otp": "1.4.1",
    "ioredis": "^5.8.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "react": "^19",
    "react-day-picker": "9.8.0",
    "react-dom": "^19",
    "react-email": "^4.2.12",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "resend": "^6.1.2",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.9",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "9.35.0",
    "eslint-config-next": "15.5.3",
    "jsdom": "^25.0.0",
    "lighthouse": "^12.0.0",
    "msw": "^2.3.0",
    "postcss": "^8.5",
    "puppeteer": "^24.22.3",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "vitest": "^2.1.9",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
